[Unit]
Description=Update cache policy
After=network.target

[Service]
User=root
Type=simple
ExecStart=/usr/bin/java -jar $serviceJar --log.location=$log_location --tier.criteria.file=$tieredCriteriaConfigFilePath --xap.workLocation=$xapWorkLocation
SuccessExitStatus=143
Restart=always
RestartSec=30


[Install] 
WantedBy=multi-user.target
